{
  "name": "ecology",
  "displayName": "Ecology",
  "description": "Syntax highlight playground code blocks in markdown",
  "version": "0.1.0",
  "publisher": "matt-tingen",
  "license": "MIT",
  "repository": {
    "url": "https://github.com/matt-tingen/vscode-ecology-extension.git"
  },
  "bugs": {
    "url": "https://github.com/matt-tingen/vscode-ecology-extension/issues"
  },
  "engines": {
    "vscode": "^1.8.0"
  },
  "categories": [
    "Other"
  ],
  "contributes": {
    "grammars": [
      {
        "scopeName": "markdown.ecology.codeblock",
        "path": "./syntaxes/codeblock.json",
        "injectTo": [
          "text.html.markdown"
        ],
        "embeddedLanguages": {
          "meta.embedded.block.ecology": "javascriptreact"
        }
      }
    ]
  },
  "dependencies": {}
}
